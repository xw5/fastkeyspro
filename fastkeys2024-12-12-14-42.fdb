1•shake•/**¶ * 判断当前系统是否是安卓¶ */¶function isAndroid() {¶  const u = window.navigator.userAgent;¶  return u.indexOf("Android") > -1 || u.indexOf("Adr") > -1;¶}¶¶/**¶ * 节流函数¶ * @param fn ¶ * @param deley ¶ * @param start ¶ * @returns ¶ */¶function throttle(fn, deley = 200, start = true) {¶  let timer = 0;¶  return function (...arg) { // 经过防抖处理的函数¶    const _this = this;¶    if (timer) {¶      return;¶    }¶    start && fn.apply(_this, arg);¶    timer = setTimeout(() => {¶      (!start) && fn.apply(_this, arg);¶      timer = 0;¶    }, deley);¶  }¶}¶¶// 手机摇一摇功能实现¶// 坑位：¶// 安卓下 和 IOS 下，加速度方向取值相反¶// 如果要使用加速度 API，当前应用则必须使用 https 协议¶// IOS 12.2 中，用户可以在手机设置中关闭掉，动作与方向的访问权限，需提示用户手动开启权限¶// IOS 13 及之后，当应用中想要使用动作与方向的访问权限时，需要请求用户授权¶// IOS 13.3 及之后，申请授权，必须用户手动触发¶class Shake {¶  constructor(errCb) {¶    this.errCb = errCb;¶    this.errCbNext = null;¶    this.init();¶  }¶¶  init() {¶    if (!window.DeviceMotionEvent) {¶      // alert("您的设备不支持速度检测");¶      this.errCb("您的设备不支持加速度检测");¶      this.errCbNext = () => {¶        this.errCb("您的设备不支持加速度检测");¶      }¶      return;¶    }¶    // 在 IOS 12 中，判断用户是否关闭了动作与方向的访问权限¶    let timer = setTimeout(() => {¶      // alert("请开启动作与方向的访问权限，否则将无法使用本应用");¶      this.errCb("请开启动作与方向的访问权限，否则将无法使用本应用");¶      this.errCbNext = () => {¶        this.errCb("请开启动作与方向的访问权限，否则将无法使用本应用");¶      }¶    }, 1000);¶    window.addEventListener("devicemotion", () => {¶      clearTimeout(timer);¶      this.errCbNext = null;¶    }, { once: true });¶  }¶¶  /*¶  * addMotion 设置监听加速变化要处理的事情¶  * cb 加速度变化后要做的处理函数¶  * return 取消事件注册¶  */¶  addMotion(cb) {¶    let fn = (e) => {¶      let { x: accelerationX, y: accelerationY, z: accelerationZ } = e.acceleration;¶      let { x: accelerationIncludingGravityX, y: accelerationIncludingGravitY, z: accelerationIncludingGravitZ } = e.accelerationIncludingGravity;¶      if (isAndroid()) { // 处理安卓取反问题¶        accelerationX = -accelerationX;¶        accelerationY = -accelerationY;¶        accelerationZ = -accelerationZ;¶        accelerationIncludingGravityX = -accelerationIncludingGravityX;¶        accelerationIncludingGravitY = -accelerationIncludingGravitY;¶        accelerationIncludingGravitZ = -accelerationIncludingGravitZ;¶      }¶      let res = {¶        acceleration: {¶          x: accelerationX,¶          y: accelerationY,¶          z: accelerationZ¶        },¶        accelerationIncludingGravity: {¶          x: accelerationIncludingGravityX,¶          y: accelerationIncludingGravitY,¶          z: accelerationIncludingGravitZ¶        }¶      }¶      cb(res);¶    };¶    // 区分 IOS 13 及之前¶    if (typeof DeviceMotionEvent.requestPermission === "function") { // IOS 13 及之后¶      DeviceMotionEvent.requestPermission()¶        .then(permissionState => {¶          if (permissionState === 'granted') {¶            // 权限允许¶            window.addEventListener("devicemotion", fn);¶            this.errCbNext = null;¶          }¶        }).catch(() => {¶          this.errCb("请开启授权否则无法使用本应用");¶          this.errCbNext = () => {¶            this.errCb("请开启授权否则无法使用本应用");¶          }¶        })¶    } else { //安卓及IOS 13之前¶      window.addEventListener("devicemotion", fn);¶      this.errCbNext = null;¶    }¶    return () => {¶      window.removeEventListener("devicemotion", fn);¶    }¶  }¶¶  /*¶  * startShake 摇一摇¶  * options : {¶  *   start:function // 开始摇一摇回调¶  *   shaking:function // 摇一摇中回调¶  *   end: function // 摇一摇结束回调¶  * }¶  */¶  startShake(options) {¶    if (this.errCbNext) {¶      this.errCbNext();¶      return;¶    }¶    const { start, shaking, end } = options;¶    let lastX = 0, lastY = 0, lastZ = 0;¶    const maxRange = 50;¶    const minRange = 5;¶    let isShake = false;¶    const unMotion = this.addMotion(throttle((e) => {¶      console.log('---- shaking ----:', e);¶      const { x, y, z } = e.acceleration;¶      const range = Math.abs(x - lastX) + Math.abs(y - lastY) + Math.abs(z - lastZ);¶      if (range > maxRange && (!isShake)) {¶        start && start(e);¶        isShake = true;¶      } else if (range > maxRange && isShake) {¶        shaking && shaking(e);¶      } else if (range < minRange && isShake) {¶        end && end(e);¶        isShake = false;¶      }¶      lastX = x;¶      lastY = y;¶      lastZ = z;¶    }));¶    return unMotion; //取消摇一摇监听¶  }¶¶}¶¶// **** 使用示例 ****¶// html 结构¶// <button id="btn">开始摇一摇</button>¶// <button id="stopBtn">停止</button>¶// <div id="info"></div>¶let btn = document.querySelector("#btn");¶let stopBtn = document.querySelector("#stopBtn");¶let info = document.querySelector("#info");¶let unShake;¶let shake = new Shake((message) => {¶  alert(message)¶})¶btn.addEventListener("click", () => {¶  unShake = shake.startShake({¶    start: () => {¶      console.log("---- start ----:", "开始摇一摇");¶      info.innerHTML = "开始摇一摇"¶    },¶    shake: () => {¶      console.log("---- start ----:", "摇一摇中");¶      info.innerHTML = "摇一摇中"¶    },¶    end: () => {¶      console.log("---- start ----:", "摇一摇结束");¶      info.innerHTML = "摇一摇结束"¶    }¶  })¶});¶stopBtn.addEventListener("click", () => {¶  if (unShake) {¶    unShake();¶  }¶})•0•0•0•0•0•1•0••js实现摇一摇•Send•1•0•1
