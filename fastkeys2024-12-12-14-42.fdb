1•sort•;选择排序::¶/**¶ * 选择排序¶ * @param arr ¶ * @returns ¶ */¶const selectSort = (arr) => {¶  const a = [...arr];¶  for (let i = 0; i < a.length; i++) {¶    const min = a¶      .slice(i + 1)¶      .reduce((acc, val, j) => (val < a[acc] ? j + i + 1 : acc), i);¶    if (min !== i) [a[i], a[min]] = [a[min], a[i]];¶  }¶  return a;¶};¶;插入排序::¶/**¶ * 插入排序¶ * @param arr ¶ * @returns ¶ */¶const insertSort = (arr) =>¶  arr.reduce((acc, x) => {¶    if (!acc.length) return [x];¶    acc.some((y, j) => {¶      if (x <= y) {¶        acc.splice(j, 0, x);¶        return true;¶      }¶      if (x > y && j === acc.length - 1) {¶        acc.splice(j + 1, 0, x);¶        return true;¶      }¶      return false;¶    });¶    return acc;¶  }, []);¶;冒泡排序::¶/**¶ * 冒泡排序¶ * @param arr ¶ * @returns ¶ */¶const bubbleSort = (arr) => {¶  let swapped = false;¶  const a = [...arr];¶  for (let i = 1; i < a.length; i++) {¶    swapped = false;¶    for (let j = 0; j < a.length - i; j++) {¶      if (a[j + 1] < a[j]) {¶        [a[j], a[j + 1]] = [a[j + 1], a[j]];¶        swapped = true;¶      }¶    }¶    if (!swapped) return a;¶  }¶  return a;¶};¶;归并排序::¶/**¶ * 归并排序¶ * @param arr ¶ * @returns ¶ */¶const mergeSort = (arr) => {¶  if (arr.length < 2) return arr;¶  const mid = Math.floor(arr.length / 2);¶  const l = mergeSort(arr.slice(0, mid));¶  const r = mergeSort(arr.slice(mid, arr.length));¶  return Array.from({ length: l.length + r.length }, () => {¶    if (!l.length) return r.shift();¶    else if (!r.length) return l.shift();¶    else return l[0] > r[0] ? r.shift() : l.shift();¶  });¶};¶;桶排序::¶/**¶ * 桶排序¶ * @param arr ¶ * @returns ¶ */¶const bucketSort = (arr, size = 5) => {¶  const min = Math.min(...arr);¶  const max = Math.max(...arr);¶  const buckets = Array.from(¶    { length: Math.floor((max - min) / size) + 1 },¶    () => []¶  );¶  arr.forEach((val) => {¶    buckets[Math.floor((val - min) / size)].push(val);¶  });¶  return buckets.reduce((acc, b) => [...acc, ...b.sort((a, b) => a - b)], []);¶};•0•0•0